            <Button fx:id="b_cat_fight" mnemonicParsing="false" style="-fx-border-color: black; -fx-border-radius: 10px; -fx-background-radius: 10px; -fx-pref-height: 30px; -fx-pref-width: 150px; -fx-background-color: C8D97D;" text="Open Cat Fight" GridPane.columnIndex="2">
