      </GridPane>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="221.0" layoutY="184.0" prefHeight="30.0" prefWidth="143.0" style="-fx-background-color: C8D97D; -fx-border-color: black; -fx-border-radius: 10px; -fx-background-radius: 10px;" text="Information:" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <Label fx:id="l_congrats" alignment="TOP_CENTER" layoutX="190.0" layoutY="35.0" prefHeight="46.0" prefWidth="798.0" style="-fx-background-color: transparent;" text="Congratulations on completing the testbench!" textAlignment="CENTER">
         <font>
            <Font name="Book Antiqua Bold" size="30.0" />
         </font>
      </Label>
      <ImageView fx:id="imgLogo" fitHeight="109.0" fitWidth="90.0" layoutX="24.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
